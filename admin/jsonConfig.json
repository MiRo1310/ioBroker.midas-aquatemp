{
	"i18n": true,
	"type": "panel",
	"items": {
		"username": {
			"type": "text",
			"label": "Username",
			"newLine": true,
			"sm": 2
		},
		"password": {
			"type": "password",
			"label": "password",
			"newLine": true,
			"sm": 2
		},

		"refresh": {
			"type": "number",
			"label": "refresh",
			"min": 60,
			"default": 60,
			"newLine": true
		},
		"deviceMac": {
			"type": "text",
			"label": "Device Mac",
			"sm": 3
